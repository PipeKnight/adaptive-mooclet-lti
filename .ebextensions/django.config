option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: django_adaptive_engine/wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: django_adaptive_engine.settings.base

packages:
  yum:
    postgresql94-devel: []

container_commands:
  01_static:
    command: "django-admin.py collectstatic --noinput"
    leader_only: true
  02_migrate:
    command: "django-admin.py migrate"
    leader_only: true
