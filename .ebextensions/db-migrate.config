container_commands:
  01_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  02_changefilepermissions:
    command: "chown wsgi:wsgi db.sqlite3"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collectstatic"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: django_adaptive_engine.settings
