<blti:title>Qualtrics LTI Bridge</blti:title>
<blti:description>
The Qualtrics LTI Bridge allows instructors to embed qualtrics quizzes in an LMS as an LTI tool.
</blti:description>
<blti:launch_url>http://qlb.vpal.io</blti:launch_url>
<blti:secure_launch_url>http://qlb.vpal.io</blti:secure_launch_url>
<blti:vendor/>
<blti:extensions platform="canvas.instructure.com">
<lticm:options name="resource_selection">
<lticm:property name="selection_width">700</lticm:property>
<lticm:property name="selection_height">500</lticm:property>
<lticm:property name="enabled">true</lticm:property>
<lticm:property name="url">http://qlb.vpal.io/qlb/select_or_create_quiz</lticm:property>
</lticm:options>
<lticm:property name="privacy_level">public</lticm:property>
<lticm:property name="domain">qlb.vpal.io</lticm:property>
</blti:extensions>
